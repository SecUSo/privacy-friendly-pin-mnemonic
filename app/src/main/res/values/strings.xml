    <!-- HelpFragment-->
    <string name="help">Help</string>
    <string name="help_overview_heading">Overview</string>
    <string name="help_tip_one">Privacy Friendly PIN Mnemonic helps you memorizing your 4-digit-PIN by presenting up to 5 strategies.</string>
    <string name="help_tip_two">Don\'t forget to practice entering your PIN multiple times for internalization.</string>
    <string name="help_tip_three">If you\'re using the word strategy be aware that most of PIN-Numpads don\'t have letters on it. You can use your phone as a crib.</string>

    <string name="help_shape_heading">Shape</string>
    <string name="help_one_highlighted">Digit is contained once</string>
    <string name="help_two_highlighted">Digit is contained twice</string>
    <string name="help_three_highlighted">Digit is contained three times</string>
    <string name="help_four_highlighted">Digit is contained four times</string>
    <string name="help_story_heading">Stories</string>
    <string name="help_story_description">Use the displayed symbols to imagine a little story or sentence to remember the PIN.</string>
    <string name="help_story_example">Example story for the PIN 4587: I held a clover in my hand, then I went to play billiards at 7p.m.</string>
    <string name="help_story_additional">The symbols are just hints, you can make up any story that helps you remembering the PIN.</string>
    <string name="help_privacy_heading">Privacy Info</string>
    <string name="help_pin_storage_heading">PIN Storage</string>
    <string name="help_pin_storage_description">Your PIN is neither stored nor transmitted</string>
    <string name="help_permissions_heading">Permissions</string>
